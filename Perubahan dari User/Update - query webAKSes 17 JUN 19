SELECT TA.id_acct, TA.inv_id, TA.full_name, TO_CHAR (TA.BIRTH_DATE, 'DD-MON-YYYY') BIRTH_DATE,
TA.KTP_NUM, TA.NPWP_NUM, TA.PASSPORT_NUM, TA.EMAIL, MOBILE_PHN, LOC_ASING, NATIONALITY, 
ADDR1, ADDR2, POSTAL_CODE, HOME_PHN, CITY, PROVINCE, COUNTRY
FROM  aksesnextg.TR_aCCOUNTS@aksesdb TA

